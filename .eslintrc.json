{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing" : ["error", "always"],
        "quotes": ["error", "double"]
    }
}
